@{
    ViewBag.Title = "Performance comparsion of HTTP/1 vs HTTP/2";
    var id = Guid.NewGuid();
}

<body class="main body-content">
    <div class="response-time-wrapper">
        <span>Load Time:</span>
        <span class="response-time">Loading...</span>
    </div>
    <div class="imgWrapper">
        @{
            for (int i = 0; i < 6; i++)
            {
                <div class="imgRow">
                    @{for (int j = 0; j < 32; j++)
                        {
                            <span>
                                <img onload="" src="@Url.Content("~/Content/images/TestImage01/TestImage [www.imagesplitter.net]-" + i + "-" + j + ".jpeg?id="+id)">
                            </span>
                        }
                    }
                </div>
            }
        }
    </div>
</body>



<script type="text/javascript">
    document.addEventListener('DOMContentLoaded', function () {
        $(window).load(function () {
            setTimeout(function () {

                var responseTime = performance.timing.loadEventEnd - performance.timing.connectStart;
                $('.response-time').text(responseTime+"ms");
            }, 0);
        });
    }, false);

</script>
